{
  "name": "toad-cache",
  "description": "LRU cache for Client or Server",
  "version": "1.0.0",
  "homepage": "https://github.com/kibertoad/toad-cache",
  "author": "Igor Savin <kibertoad@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/toad-cache.git"
  },
  "bugs": {
    "url": "https://github.com/kibertoad/toad-cache/issues"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "license": "MIT",
  "source": "src/lru.js",
  "main": "dist/toad-cache.cjs",
  "module": "dist/toad-cache.esm.js",
  "type": "module",
  "sourceType": "module",
  "types": "lru.d.ts",
  "engines": {
    "node": ">=12"
  },
  "engineStrict": true,
  "scripts": {
    "build": "npm run lint && rm -rf dist/* && npm run rollup && npm run mocha",
    "benchmark": "node benchmark.js",
    "changelog": "auto-changelog -p",
    "lint": "eslint *.js src/*.js test/*.js",
    "lint:fix": "eslint --fix *.js src/*.js test/*.js",
    "format": "prettier --write \"{src,test}/**/*.js\" benchmark.js ",
    "rollup": "rollup --config",
    "test": "vitest",
    "test:coverage": "npm run rollup && npm run test -- --coverage",
    "test:ci": "npm run rollup && npm run lint && npm run test:coverage",
    "types": "npx -p typescript tsc src/lru.js --declaration --allowJs --emitDeclarationOnly --outDir ."
  },
  "devDependencies": {
    "@vitest/coverage-c8": "0.29.8",
    "@rollup/plugin-terser": "^0.4.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "precise": "^2.0.1",
    "rollup": "^3.19.1",
    "vitest": "0.29.8",
    "typescript": "^5.0.2"
  },
  "keywords": [
    "LRU",
    "cache",
    "client",
    "server",
    "least",
    "recently",
    "used"
  ]
}
